From 9aad5d478d42cbbc4028e2d5ab882ca5733989b1 Mon Sep 17 00:00:00 2001
From: Paul Yim <paul.j.yim@gmail.com>
Date: Wed, 24 Mar 2021 15:05:24 -0400
Subject: [PATCH] Add -fpermissive to CFLAGS in linux command template

---
 qtwebengine/src/3rdparty/gn/build/build_linux.ninja.template | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/qtwebengine/src/3rdparty/gn/build/build_linux.ninja.template b/qtwebengine/src/3rdparty/gn/build/build_linux.ninja.template
index e59854b189..d079569e51 100644
--- a/qtwebengine/src/3rdparty/gn/build/build_linux.ninja.template
+++ b/qtwebengine/src/3rdparty/gn/build/build_linux.ninja.template
@@ -1,11 +1,11 @@
 rule cc
-  command = $cc -MMD -MF $out.d $defines $includes $cflags $cflags_c -c $in -o $out
+  command = $cc -MMD -MF $out.d $defines $includes $cflags -fpermissive $cflags_c -c $in -o $out
   description = CC $out
   depfile = $out.d
   deps = gcc
 
 rule cxx
-  command = $cxx -MMD -MF $out.d $defines $includes $cflags $cflags_cc -c $in -o $out
+  command = $cxx -MMD -MF $out.d $defines $includes $cflags -fpermissive $cflags_cc -c $in -o $out
   description = CXX $out
   depfile = $out.d
   deps = gcc
-- 
