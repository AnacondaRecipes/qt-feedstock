Subject: [PATCH] Add -fpermissive to CFLAGS to treat third-party errors as
 warnings

---
 qtwebengine/src/3rdparty/gn/build/gen.py | 1 +
 1 file changed, 1 insertion(+)

diff --git a/qtwebengine/src/3rdparty/gn/build/gen.py b/qtwebengine/src/3rdparty/gn/build/gen.py
index b09d32ce84..76ad0c5ba4 100755
--- a/qtwebengine/src/3rdparty/gn/build/gen.py
+++ b/qtwebengine/src/3rdparty/gn/build/gen.py
@@ -311,6 +311,7 @@ def WriteGNNinja(path, platform, host, options):
   libs = []
 
   if not platform.is_msvc():
+    cflags.extend(['-fpermissive'])
     if options.debug:
       cflags.extend(['-O0', '-g'])
     else:
